// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id          String   @id @default(uuid())
  actor_id    String
  group       String
  action_id   String
  target_id   String
  target_name String
  location    String?
  occurred_at DateTime
  metadata    Json
}